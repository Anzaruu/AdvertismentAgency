<Window x:Class="ProjectAdvert.WindowEmploye"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProjectAdvert"
        mc:Ignorable="d"
        Title="WindowEmploye" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="0.2*"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Grid Grid.Row="0">
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Button x:Name="ToPageCityBtn" Grid.Column="0" Content="Город" FontSize="25" Click="ToPageCityBtn_Click"/>
            <Button x:Name="ToPageClientBtn" Grid.Column="1" Content="Клиент" FontSize="25" Click="ToPageClientBtn_Click"/>
            <Button x:Name="ToPageChequeBtn" Grid.Column="2" Content="Чек" FontSize="25" Click="ToPageChequeBtn_Click"/>
            <Button x:Name="ToPageSavedChequeBtn" Grid.Column="3" Content="Сохраненные чеки" FontSize="22" Click="ToPageSavedChequeBtn_Click"/>
        </Grid>
        <Frame x:Name="PageMoving" Grid.Row="1" NavigationUIVisibility="Hidden" />
    </Grid>
</Window>
